def siamese(): # function for siamese
    print("SIAMESE\n")
    SME = Species("Siamese", "Felis", "Felidae", "Carnivora", "Mammalia", "Chordata", "Animalia", "Eukarya")

    print("""                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
                                                                                                 ████████                                                 
                               ████████                                                       ████████████                                                
                              ████████████                                                  ██████████████                                                
                              ██████████████                                              ████████████████                                                
