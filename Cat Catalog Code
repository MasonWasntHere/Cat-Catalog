def orange_tabby_cat(): # function for orange tabby cat'
    print("ORANGE TABBY CAT\n")
    OTC = Species("Orange tabby cat", "Felis", "Felidae", "Carnivora", "Mammalia", "Chordata", "Animalia", "Eukarya")

