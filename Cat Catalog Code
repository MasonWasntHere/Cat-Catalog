    PSN.display_catalog() # display catalog of species

def snow_leopard(): # function for snow leopard
    print("SNOW LEOPARD\n")
    SLD = Species("Snow Leopard", "Panthera", "Felidae", "Carnivora", "Mammalia", "Chordata", "Animalia", "Eukarya")

    print("""                                                                                                                                                                                                                                                                                                                                                                                                                   
