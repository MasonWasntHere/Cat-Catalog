def black_footed_cat(): # function for black footed cat
    print("BLACK-FOOTED CAT\n")
    BFC = Species("Black-footed cat", "Felis", "Felidae", "Carnivora", "Mammalia", "Chordata", "Animalia", "Eukarya")

