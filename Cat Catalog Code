
class Domain: # class for domain, the highest taxonomic rank
    def __init__(self, domain):
        self.domain = domain
        self.domaininfo = self.domainDescription()

    def domainDescription(self): # function to return domain information based on the domain name
        if self.domain == "Eukarya":
            return "Eukarya is a domain that contains organisms with eukaryotic cells, which have a defined nucleus."
        else:
            return "Unknown domain"

class Kingdom(Domain): # class for kingdom, inherits from Domain
    def __init__(self, kingdom, domain):
        super().__init__(domain)
        self.kingdom = kingdom
        self.kingdominfo = self.kingdomDescription()

    def kingdomDescription(self):
        if self.kingdom == "Animalia":
            return "The Animalia kingdom includes multicellular, eukaryotic organisms that eat other organisms, can move, and usually reproduce sexually."
        else:
            return "Unknown kingdom"

class Phylum(Kingdom): # class for phylum, inherits from Kingdom
    def __init__(self, phylum, kingdom, domain):
        super().__init__(kingdom, domain)
        self.phylum = phylum
        self.phyluminfo = self.phylumDescription()

    def phylumDescription(self):
        if self.phylum == "Chordata":
            return "The Chordata phylum consists of animals that have, at some stage of their life, a notochord, a dorsal nerve cord, pharyngeal slits, and a post-anal tail."
        else:
            return "Unknown phylum"

class Class(Phylum): # class for class, inherits from Phylum
    def __init__(self, class_, phylum, kingdom, domain):
        super().__init__(phylum, kingdom, domain)
        self.class_ = class_
        self.classinfo = self.classDescription()

    def classDescription(self):
        if self.class_ == "Mammalia":
            return "The Mammalia class includes warm-blooded animals that have hair or fur, give live birth (except for monotremes), and nurse their young with milk."
        else:
            return "Unknown class"

class Order(Class): # class for order, inherits from Class
    def __init__(self, order, class_, phylum, kingdom, domain):
        super().__init__(class_, phylum, kingdom, domain)
        self.order = order
        self.orderinfo = self.orderDescription()

    def orderDescription(self):
        if self.order == "Carnivora":
            return "The Carnivora order includes animals that primarily eat meat, have sharp teeth and claws for hunting, and include species like cats, dogs, and bears."
        else:
            return "Unknown order"

class Family(Order): # class for family, inherits from Order
    def __init__(self, family, order, class_, phylum, kingdom, domain):
        super().__init__(order, class_, phylum, kingdom, domain)
        self.family = family
        self.familyinfo = self.familyDescription()

    def familyDescription(self): # function to return family information based on the family name
        if self.family == "Felidae":
            return "The Felidae family includes carnivorous mammals known as cats, characterized by retractable claws, sharp teeth, and keen hunting skills."
        else:
            return "Unknown family"

class Genus(Family): # class for genus, inherits from Family
    def __init__(self, genus, family, order, class_, phylum, kingdom, domain):
        super().__init__(family, order, class_, phylum, kingdom, domain)
        self.genus = genus
        self.genusinfo = self.genusDescription()

    def genusDescription(self): # function to return genus information based on the genus name
        if self.genus == "Felis": # if the genus is Felis, return the description for Felis
            return "The Felis genus includes small to medium-sized wild and domestic cats, known for their agility, sharp claws, and solitary hunting behavior."
        elif self.genus == "Panthera": # if the genus is Panthera, return the description for Panthera
            return "The Panthera genus includes large cats like lions, tigers, leopards, and snow leopards, known for their powerful builds and ability to roar."
        elif self.genus == "Prionailurus": # if the genus is Prionailurus, return the description for Prionailurus
            return "The Prionailurus genus includes small wild cats like the fishing cat and rusty-spotted cat, adapted to various environments and known for their unique hunting skills."
        else:
            return "Unknown genus"

class Species(Genus): # class for species, inherits from Genus
    def __init__(self, species, genus, family, order, class_, phylum, kingdom, domain):
        super().__init__(genus, family, order, class_, phylum, kingdom, domain)
        self.species = species
        self.speciesinfo = self.speciesDescription()

    def speciesDescription(self): # function to return species information based on the species name
        species_dict = {
            "Rusty-spotted cat": "The rusty-spotted cat is one of the world's smallest wild cats, native to India and Sri Lanka.",
            "Orange tabby cat": "The orange tabby cat is a domesticated feline known for its vibrant coat pattern and friendly demeanor.",
            "Black-footed cat": "The black-footed cat is a small wild cat native to southern Africa, renowned for its incredible hunting efficiency.",
            "Fishing cat": "The fishing cat is a wild feline adept at swimming and primarily preys on fish in wetland habitats.",
            "Chartreux": "The Chartreux is a rare breed of domestic cat from France, known for its robust build and blue-gray fur.",
            "Turkish Van": "The Turkish Van is a unique breed distinguished by its love of swimming and striking coat pattern.",
            "Munchkin": "The Munchkin cat is famous for its short legs due to a genetic mutation, but it remains energetic and playful.",
            "Persian": "The Persian cat is a long-haired breed known for its distinctive face and calm temperament.",
            "Snow Leopard": "The snow leopard is a large wild cat native to mountainous regions of Central and South Asia, known for its thick fur and elusive nature.",
            "Kurilian Bobtail": "The Kurilian Bobtail is a rare breed from Russia, notable for its short, pom-pom-like tail.",
            "Siamese": "The Siamese cat is a social and vocal breed known for its striking blue eyes and pointed coat pattern.",
            "Tiffany cat": "Also known as the Chantilly-Tiffany, this cat is famous for its luxurious semi-long coat and affectionate personality."
        }
        return species_dict.get(self.species, "Unknown species")

    def display_catalog(self): # function to display the entire catalog of said species information
        print(f"Domain: {self.domain}\n{self.domaininfo}\n")
        print(f"Kingdom: {self.kingdom}\n{self.kingdominfo}\n")
        print(f"Phylum: {self.phylum}\n{self.phyluminfo}\n")
        print(f"Class: {self.class_}\n{self.classinfo}\n")
        print(f"Order: {self.order}\n{self.orderinfo}\n")
        print(f"Family: {self.family}\n{self.familyinfo}\n")
        print(f"Genus: {self.genus}\n{self.genusinfo}\n")
        print(f"Species: {self.species}\n{self.speciesinfo}\n")

def rusty_spotted_cat(): # function for rusty spotted cat
    print("RUSTY-SPOTTED CAT\n")
    RSC = Species("Rusty-spotted cat", "Prionailurus", "Felidae", "Carnivora", "Mammalia", "Chordata", "Animalia", "Eukarya")

