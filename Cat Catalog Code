                                                          █▓███                                                                                           
""")

    FSC.display_catalog() # display catalog of species

def display_catalog(cat): # function to display catalog of species
    cat.display_catalog() 

def create_cat_function(species_name, genus, family, order, class_, phylum, kingdom, domain):
    def cat_function():
        cat = Species(species_name, genus, family, order, class_, phylum, kingdom, domain)
        display_catalog(cat)
    return cat_function

def title_screen(): # function for title screen
    print("           ████████                                                                         ")
    print("          ███    ████              ███                                                      ")
    print("         ███             ██████   ██████                                                    ")
    print("         ██            ███   ███   ███                                                      ")
    print("         ██                █████   ███                                                      ")
    print("         ███      ███  █████  ██   ███                                                      ")
    print("          ██████████   ███   ███   ███                                                      ")
    print("             █████      █████████   ████                                                    ")
    print("                                                                                            ")
    print("                                                                                            ")
    print("                                                                                            ")
    print("                                                                                            ")
    print("                                                                                            ")
    print("                                                                                            ")
    print("                    ███                                                                     ")
    print("                 █████████                ██                ██                              ")
    print("                ███     ███              ████               ██                              ")
    print("               ███            ████████  ██████  █████████   ██    ████████    █████████     ")
    print("               ███                  ███   ██          ███   ██   ███    ███  ███    ███     ")
    print("               ███             ████████   ██     ████████   ██  ███     ███  ███     ██     ")
    print("                ███     ███   ██    ███   ██    ██    ███   ██   ███    ███  ███    ███     ")
    print("                 █████████    █████████   ████  █████████   ██    ████████    █████████     ")
    print("                                                                                     ██     ")
    print("                                                                              █████████     ")
    print("                                                                                ████        ")
    print("")                                                                                                           
    print("Welcome to the Cat Catalog!")
    print("")

def display_menu(): # function to display menu of species
    print("Please select one of the following cats to learn about them!")
    print("")
    print("1. Rusty-spotted cat")
    print("2. Black-footed cat")
    print("3. Orange tabby cat")
    print("4. Chartreux")
    print("5. Turkish Van")
    print("6. Munchkin")
    print("7. Persian")
    print("8. Snow Leopard")
    print("9. Kurilian Bobtail")
    print("10. Siamese")
    print("11. Tiffany cat")
    print("12. Fishing cat")
    print("")

def main():
    title_screen()
    while True:
        display_menu()
        choice = input("Enter the number of the cat you want to learn about: ")

        if choice == "1": # rusty_spotted_cat
            rusty_spotted_cat()
        elif choice == "2": # black_footed_cat
            black_footed_cat()
        elif choice == "3": # orange_tabby_cat
            orange_tabby_cat()
        elif choice == "4": # chartreux
            chartreux()
        elif choice == "5": # turkish_van
            turkish_van()
        elif choice == "6": # munchkin
            munchkin()
        elif choice == "7": # persian
            persian()
        elif choice == "8": # snow_leopard
            snow_leopard()
        elif choice == "9": # kurilian bobtail
            kurilian_bobtail()
        elif choice == "10": # siamese
            siamese()
        elif choice == "11": #  tiffany_cat
            tiffany_cat()
        elif choice == "12": # fishing_cat
            fishing_cat()
        else:
            print("Invalid choice. Please select a valid number.")
            print("")
            continue

        another = input("Would you like to learn about another cat? (y/n): ").strip().lower()
        if another != 'y':
            print("Thank you for using the Cat Catalog. Goodbye!")
            break

main()
