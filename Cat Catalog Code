    MCK.display_catalog() # display catalog of species

def persian(): # function for persian
    print("PERSIAN\n")
    PSN = Species("Persian", "Felis", "Felidae", "Carnivora", "Mammalia", "Chordata", "Animalia", "Eukarya")

