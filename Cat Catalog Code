    OTC.display_catalog() # display catalog of species

def chartreux(): # function for chartreux
    print("CHARTREUX\n")
    CTX = Species("Chartreux", "Felis", "Felidae", "Carnivora", "Mammalia", "Chordata", "Animalia", "Eukarya")

    print("""                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
