    SME.display_catalog() # display catalog of species

def tiffany_cat(): # function for tiffany cat
    print("TIFFANY CAT\n")
    TYC = Species("Tiffany cat", "Felis", "Felidae", "Carnivora", "Mammalia", "Chordata", "Animalia", "Eukarya")

