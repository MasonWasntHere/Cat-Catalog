def kurilian_bobtail(): # function for kurilian bobtail
    print("KURILIAN BOBTAIL\n")
    KBT = Species("Kurilian Bobtail", "Felis", "Felidae", "Carnivora", "Mammalia", "Chordata", "Animalia", "Eukarya")

